{
  "name": "bifrost-plugin",
  "version": "1.0.0",
  "description": "Plugin installer for bifrost projects",
  "type": "module",
  "bin": {
    "bifrost-plugin": "./dist/index.js"
  },
  "scripts": {
  "copy-registry": "bun copy-registry.js",
  "build:tsup": "tsup src/index.ts --format esm --dts --clean",
  "build": "npm run copy-registry && npm run build:tsup",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "bifrost",
    "plugin",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "prompts": "^2.4.2",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/prompts": "^2.4.9",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1"
  }
}